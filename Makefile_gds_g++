CXX = g++
CFLAGS = -O

CLMOBJ = tclm_gds.o  

LULCOBJ = tbiome_gds.o tlcluc_gds.o tdisturb_gds.o

TEMOBJ = ttem_gds.o atms_gds.o penmon_gds.o tveg_gds.o tmcrb_gds.o \
tsoil_gds.o humnact_gds.o

ELMNTOBJ = elmnt_gds.o telm_gds.o latdat_gds.o \
tclmdat_gds.o tco2dat_gds.o \
lulcdat_gds.o tmaxcohortdat_gds.o \
ttemdat_gds.o telvdat_gds.o tsoldat_gds.o disturbdat_gds.o \
atmdepdat_gds.o tatmdep_gds.o \
nfertdat_gds.o tnfert_gds.o

OBJ =  tprocessXML_gds.o

xtem_gds_g++ : ${CLMOBJ} ${LULCOBJ} ${TEMOBJ} ${ELMNTOBJ} ${OBJ} xtem_gds.cpp 
	${CXX} ${CFLAGS} -o xtem_gds_g++ ${CLMOBJ} ${LULCOBJ} ${TEMOBJ} ${ELMNTOBJ} ${OBJ} xtem_gds.cpp

xtem_gds_gofile_g++ : ${CLMOBJ} ${LULCOBJ} ${TEMOBJ} ${ELMNTOBJ} ${OBJ} xtem_gds_gofile.cpp
	${CXX} ${CFLAGS} -o xtem_gds_gofile_g++ ${CLMOBJ} ${LULCOBJ} ${TEMOBJ} ${ELMNTOBJ} ${OBJ} xtem_gds_gofile.cpp

atms_gds.o : atms_gds.cpp atms_gds.h
	${CXX} ${CFLAGS} -c atms_gds.cpp

